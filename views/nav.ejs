    <script>
        document.querySelectorAll('.reg')[0].addEventListener('click', function(e){
        })
        let darkmode_switch = true
        $('.dark-btn').on('click',function(){
            if (darkmode_switch==true){
                console.log('white mode')
                $(".nav").addClass("dark");
                $(".dark-btn").html("☀️일반모드");
                darkmode_switch = false
            }
            else {
